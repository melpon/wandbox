{
	"name": "canine2",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"r2:develop": "wrangler r2 object put wandbox-resources/develop/clangd.wasm --file=public-r2/clangd.wasm",
		"r2:master": "wrangler r2 object put wandbox-resources/master/clangd.wasm --file=public-r2/clangd.wasm",
		"build": "remix vite:build",
		"deploy:develop": "npm run build && npm run r2:develop && wrangler deploy --env develop",
		"deploy:master": "npm run build && npm run r2:master && wrangler deploy --env master",
		"dev": "remix vite:dev",
		"lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
		"typecheck": "tsc",
		"typegen": "wrangler types"
	},
	"dependencies": {
		"@codemirror/autocomplete": "^6.18.6",
		"@codemirror/commands": "^6.8.0",
		"@codemirror/lang-cpp": "^6.0.2",
		"@codemirror/lang-java": "^6.0.1",
		"@codemirror/lang-javascript": "^6.2.3",
		"@codemirror/lang-php": "^6.0.1",
		"@codemirror/lang-python": "^6.1.7",
		"@codemirror/lang-rust": "^6.0.1",
		"@codemirror/language": "^6.10.8",
		"@codemirror/legacy-modes": "^6.4.3",
		"@codemirror/lint": "^6.8.4",
		"@codemirror/search": "^6.5.9",
		"@lezer/highlight": "^1.2.1",
		"@reduxjs/toolkit": "^2.5.1",
		"@remix-run/cloudflare": "^2.15.3",
		"@remix-run/cloudflare-pages": "^2.15.3",
		"@remix-run/react": "^2.15.3",
		"@replit/codemirror-vim": "^6.2.1",
		"ansi_up": "^6.0.2",
		"bootstrap": "^5.3.3",
		"can-ndjson-stream": "^1.0.2",
		"date-fns": "^4.1.0",
		"i18next": "^23.16.8",
		"i18next-browser-languagedetector": "^8.0.4",
		"immer": "^10.1.1",
		"isbot": "^4.1.0",
		"marked": "^15.0.7",
		"react": "^18.2.0",
		"react-bootstrap": "^2.10.9",
		"react-bootstrap-icons": "^1.11.5",
		"react-dom": "^18.2.0",
		"react-i18next": "^15.4.1",
		"react-redux": "^9.2.0",
		"remix-i18next": "^6.4.1",
		"vscode-jsonrpc": "^8.2.1",
		"vscode-languageserver-protocol": "^3.17.5"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20250214.0",
		"@remix-run/cloudflare-workers": "^2.15.3",
		"@remix-run/dev": "^2.15.3",
		"@types/react": "^18.2.20",
		"@types/react-dom": "^18.2.7",
		"@typescript-eslint/eslint-plugin": "^6.7.4",
		"@typescript-eslint/parser": "^6.7.4",
		"autoprefixer": "^10.4.19",
		"eslint": "^8.38.0",
		"eslint-import-resolver-typescript": "^3.8.3",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-jsx-a11y": "^6.7.1",
